<script type="text/javascript">

    function funjc(){
        var ccValue1 =  document.getElementById('vp-view-VC0201-001_RS0010_acct1').value;
        var ccValue2 =  document.getElementById('vp-view-VC0201-001_RS0010_acct2').value;
        var ccValue3 =  document.getElementById('vp-view-VC0201-001_RS0010_acct3').value;
        var ccValue4 =  document.getElementById('vp-view-VC0201-001_RS0010_acct4').value;
        var ccValue  = ccValue1+ccValue2+ccValue3+ccValue4;
        if(ccValue1 !=null && ccValue1 !="" &&ccValue2 !=null && ccValue2 !="" &&ccValue3 !=null && ccValue3 !="" &&ccValue4 !=null && ccValue4 !=""){
            ccchecker(ccValue);
        }

    }

    function valid_credit_card(value) {
        if (/[^0-9-\s]+/.test(value))
            return false;
        var nCheck = 0, bEven = false;
        value = value.replace(/\D/g, "");

        for (var n = value.length - 1; n >= 0; n--) {
            var cDigit = value.charAt(n),
                    nDigit = parseInt(cDigit, 10);

            if (bEven && (nDigit *= 2) > 9) nDigit -= 9;

            nCheck += nDigit;
            bEven = !bEven;
        }
        return (nCheck % 10) == 0;
    }

    function ccchecker(ccValue) {
        if (ccValue.length == 0) {
            document.getElementById("vp-view-VC0201-001_RS0010_acct1").innerHTML = "";
            document.getElementById("vp-view-VC0201-001_RS0010_acct2").innerHTML = "";
            document.getElementById("vp-view-VC0201-001_RS0010_acct3").innerHTML = "";
            document.getElementById("vp-view-VC0201-001_RS0010_acct4").innerHTML = "";
            return;
        } else {
            var isCreditCard = valid_credit_card(ccValue)
            if (!isCreditCard) {
                document.getElementById("isZhengqueCC").innerHTML = "<font color='red' ><strong>無効カードの疑いがある。</strong></font>";
                document.getElementById("vp-view-VC0201-001_RS0010_acct1").value = "";
                document.getElementById("vp-view-VC0201-001_RS0010_acct2").value = "";
                document.getElementById("vp-view-VC0201-001_RS0010_acct3").value = "";
                document.getElementById("vp-view-VC0201-001_RS0010_acct4").value = "";
                return false;
            } else {
                document.getElementById("isZhengqueCC").innerHTML = "〔半角数字16桁〕";
            }
        }
    }
</script>
